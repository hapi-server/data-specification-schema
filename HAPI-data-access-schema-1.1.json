{
   "HAPI" : {
      "description" : "HAPI Version",
      "id" : "/HAPI",
      "pattern" : "^1.1$",
      "type" : "string"
   },
   "HAPIDateTime" : {
      "anyOf" : [
         {
            "pattern" : "^[0-9]{4}Z?$",
            "type" : "string"
         },
         {
            "pattern" : "^[0-9]{4}-[0-9]{3}T?Z?$",
            "type" : "string"
         },
         {
            "pattern" : "^[0-9]{4}-[0-9]{1,3}T[0-9]{1,2}Z?$",
            "type" : "string"
         },
         {
            "pattern" : "^[0-9]{4}-[0-9]{1,3}T[0-9]{1,2}:[0-9]{1,2}Z?$",
            "type" : "string"
         },
         {
            "pattern" : "^[0-9]{4}-[0-9]{1,3}T[0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2}\\.?Z?$",
            "type" : "string"
         },
         {
            "pattern" : "^[0-9]{4}-[0-9]{1,3}T[0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2}\\.[0-9]*Z?$",
            "type" : "string"
         },
         {
            "pattern" : "^[0-9]{4}-[0-9]{1,2}-[0-9]{1,2}T?Z?$",
            "type" : "string"
         },
         {
            "pattern" : "^[0-9]{4}-[0-9]{1,2}-[0-9]{1,2}T[0-9]{1,2}Z?$",
            "type" : "string"
         },
         {
            "pattern" : "^[0-9]{4}-[0-9]{1,2}-[0-9]{1,2}T[0-9]{1,2}:[0-9]{1,2}Z?$",
            "type" : "string"
         },
         {
            "pattern" : "^[0-9]{4}-[0-9]{1,2}-[0-9]{1,2}T[0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2}\\.?Z?$",
            "type" : "string"
         },
         {
            "pattern" : "^[0-9]{4}-[0-9]{1,2}-[0-9]{1,2}T[0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2}\\.[0-9]*Z?$",
            "type" : "string"
         }
      ],
      "description" : "Regular expressions to handle all allowed HAPI date/time formats (a subset of ISO 8601)",
      "id" : "/HAPIDateTime"
   },
   "HAPIStatus" : {
      "description" : "Request status",
      "id" : "/HAPIStatus",
      "properties" : {
         "HAPI" : {
            "$ref" : "/HAPI"
         },
         "required" : [
            "HAPI",
            "status"
         ],
         "status" : {
            "description" : "Request status",
            "properties" : {
               "code" : {
                  "description" : "HAPI request status code",
                  "enum" : [
                     1200,
                     1400,
                     1401,
                     1402,
                     1403,
                     1404,
                     1405,
                     1406,
                     1407,
                     1408,
                     1409,
                     1410,
                     1500,
                     1501
                  ],
                  "type" : "integer"
               },
               "message" : {
                  "description" : "HAPI request status message",
                  "type" : "string"
               }
            },
            "required" : [
               "code",
               "message"
            ],
            "type" : "object"
         }
      },
      "title" : "info",
      "type" : "object"
   },
   "capabilities" : {
      "description" : "Server /capabilities response",
      "properties" : {
         "HAPI" : {
            "$ref" : "/HAPI"
         },
         "outputFormats" : {
            "additionalItems" : true,
            "description" : "Output formats",
            "items" : {
               "type" : "string"
            },
            "minItems" : 1,
            "type" : "array",
            "uniqueItems" : true
         },
         "status" : {
            "$ref" : "/HAPIStatus"
         }
      },
      "required" : [
         "HAPI",
         "outputFormats",
         "status"
      ],
      "title" : "capabilities",
      "type" : "object"
   },
   "catalog" : {
      "description" : "Server /catalog response",
      "properties" : {
         "HAPI" : {
            "$ref" : "/HAPI"
         },
         "catalog" : {
            "additionalItems" : true,
            "description" : "Catalog of datasets",
            "items" : {
               "required" : [
                  "id"
               ],
               "type" : "object"
            },
            "minItems" : 1,
            "properties" : {
               "id" : {
                  "description" : "Dataset ID",
                  "type" : "string",
                  "uniqueItems" : true
               },
               "title" : {
                  "description" : "Optional dataset title",
                  "type" : "string",
                  "uniqueItems" : true
               }
            },
            "type" : "array",
            "uniqueItems" : true
         },
         "status" : {
            "$ref" : "/HAPIStatus"
         }
      },
      "required" : [
         "HAPI",
         "status",
         "catalog"
      ],
      "title" : "catalog",
      "type" : "object"
   },
   "info" : {
      "dependencies" : {
         "sampleStartDate" : [
            "sampleStopDate"
         ],
         "sampleStopDate" : [
            "sampleStartDate"
         ]
      },
      "description" : "Server /info response",
      "properties" : {
         "HAPI" : {
            "$ref" : "/HAPI"
         },
         "cadence" : {
            "description" : "Time difference between records as an ISO 8601 duration. This is meant as a guide to the nominal cadence of the data and not necessarily a precise statement about the time between measurements.",
            "type" : "string"
         },
         "contact" : {
            "description" : "the name of a person to contact for more information about the dataset",
            "type" : "string"
         },
         "contactID" : {
            "description" : "the identifier for the contact person in an external data system",
            "type" : "string"
         },
         "creationDate" : {
            "$ref" : "/HAPIDateTime",
            "description" : "the date and time when this dataset was created"
         },
         "description" : {
            "description" : "a brief description of the dataset",
            "type" : "string"
         },
         "format" : {
            "type" : "string"
         },
         "modificationDate" : {
            "$ref" : "/HAPIDateTime",
            "description" : "the date and time when this dataset was last modified"
         },
         "parameters" : {
            "additionalItems" : true,
            "description" : "Parameters in dataset",
            "items" : {
               "properties" : {
                  "bins" : {
                     "description" : "Bins for components of a with a size having at least one element greater than 1",
                     "items" : {
                        "properties" : {
                           "centers" : {
                              "description" : "The centers of each bin",
                              "items" : {
                                 "type" : "number"
                              },
                              "minItems" : 1,
                              "type" : "array"
                           },
                           "description" : {
                              "description" : "A brief comment explaining what the bins represent",
                              "type" : "string"
                           },
                           "name" : {
                              "description" : "Name for the dimension (e.g., 'Frequency')",
                              "type" : "string"
                           },
                           "ranges" : {
                              "description" : "The boundaries for each bin",
                              "items" : {
                                 "items" : {
                                    "type" : "number"
                                 },
                                 "minItems" : 1,
                                 "type" : "array"
                              },
                              "type" : "array"
                           },
                           "units" : {
                              "description" : "The units for the bins (e.g., 'Hz')",
                              "type" : "string"
                           }
                        },
                        "required" : [
                           "name",
                           "centers"
                        ],
                        "type" : "object"
                     },
                     "type" : "array"
                  },
                  "fill" : {
                     "description" : "Parameter fill",
                     "type" : [
                        "string",
                        "null"
                     ]
                  },
                  "length" : {
                     "description" : "Number of characters in parameter (isotime and string parameters only)",
                     "type" : "integer"
                  },
                  "name" : {
                     "description" : "Parameter name",
                     "type" : "string"
                  },
                  "size" : {
                     "description" : "Parameter size",
                     "items" : {
                        "type" : "integer"
                     },
                     "minItems" : 1,
                     "minimum" : 1,
                     "type" : "array"
                  },
                  "type" : {
                     "description" : "Parameter type",
                     "enum" : [
                        "string",
                        "double",
                        "integer",
                        "isotime"
                     ],
                     "type" : "string"
                  },
                  "units" : {
                     "description" : "Parameter units",
                     "type" : [
                        "string",
                        "null"
                     ]
                  }
               },
               "required" : [
                  "name",
                  "type",
                  "units",
                  "fill"
               ],
               "type" : "object"
            },
            "minItems" : 1,
            "type" : "array",
            "uniqueItems" : true
         },
         "resourceID" : {
            "description" : "an external identifier by which other datasets might know this dataset, such as a DOI or SPASE ID",
            "type" : "string"
         },
         "resourceURL" : {
            "description" : "a link to more information about the dataset",
            "type" : "string"
         },
         "sampleStartDate" : {
            "$ref" : "/HAPIDateTime",
            "description" : "start of a sample time period for a dataset, where the time period should contain a manageable, representative example of valid, non-fill data"
         },
         "sampleStopDate" : {
            "$ref" : "/HAPIDateTime",
            "description" : "stop date/time associated with sampleStartDate"
         },
         "startDate" : {
            "$ref" : "/HAPIDateTime",
            "description" : "start date/time of parameter data"
         },
         "status" : {
            "$ref" : "/HAPIStatus"
         },
         "stopDate" : {
            "$ref" : "/HAPIDateTime",
            "description" : "start date/time of parameter data"
         }
      },
      "required" : [
         "HAPI",
         "status",
         "startDate",
         "stopDate",
         "parameters"
      ],
      "title" : "info",
      "type" : "object"
   }
}
